# Django Settings
# SECURITY WARNING: Generate a new secret key for production!
# You can generate one using: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=your-secret-key-here

# Portbro OAuth2 Configuration
PORTBRO_CLIENT_ID=your-portbro-client-id
PORTBRO_CLIENT_SECRET=your-portbro-client-secret
PORTBRO_TOKEN_URL=https://portbro.com/o/token/
PORTBRO_VPN_AUTH_URL=https://portbro.com/vpn/auth/
PORTBRO_SCOPE=read

# JWT RSA Keys
# Option 1: Provide the public key directly (for single-line, use \n for newlines)
# JWT_RSA_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEA...\n-----END PUBLIC KEY-----"

# Option 2: Provide path to a file containing the public key (recommended)
# JWT_RSA_PUBLIC_KEY_FILE=/path/to/public_key.pem

# Private key (only needed for local token generation in development/testing)
# WARNING: In production, tokens should be generated by portbro.com, not locally
# JWT_RSA_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nMIIEvQIBADANBgkqhkiG9w0BAQEFAASCBKcwggSjAgEAAoIBAQC...\n-----END PRIVATE KEY-----"
# JWT_RSA_PRIVATE_KEY_FILE=/path/to/private_key.pem

# n8n API Key for webhook authentication
N8N_API_KEY=your-n8n-api-key-here

# Optional: JWT Configuration (defaults shown)
# PARENT_JWKS_URL=https://portbro.com/o/jwks/
# PARENT_ISSUER=portbro.com
# PARENT_AUDIENCE=vpn-nodes
